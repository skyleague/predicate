{
  "name": "@skyleague/predicate",
  "description": "",
  "version": "1.0.0",
  "license": "MIT",
  "types": "./.dist/index.d.ts",
  "sideEffects": false,
  "node-standards": {
    "extends": [
      "library",
      "docusaurus"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "homepage": "https://skyleague.github.io/node-standards/",
  "repository": {
    "type": "git",
    "url": "https://github.com/skyleague/node-standards.git"
  },
  "files": [
    ".dist",
    "package.json"
  ],
  "scripts": {
    "build": "node --import tsx/esm build.config.ts",
    "build:docs": "npm run --workspace=docs build",
    "check:coverage": "vitest run --coverage=true",
    "check:project": "node-standards lint",
    "check:types": "tsc -p tsconfig.json",
    "format": "prettier \"**/*.{ts,js,json,yml,yaml,md}\" --write",
    "lint": "eslint \"./{src,test,typings}/**/*.ts\" --cache --no-eslintrc --config .eslintrc.cjs",
    "lint:fix": "npm run lint -- --fix",
    "package": "rm -rf .dist && npm run build",
    "prepare": "husky || true",
    "test": "vitest run"
  },
  "dependencies": {
    "@skyleague/jsonpath": "^3.1.1",
    "tslib": "^2.6.2",
    "type-fest": "^4.10.2"
  },
  "devDependencies": {
    "@skyleague/axioms": "^3.6.0",
    "@skyleague/node-standards": "^5.1.7",
    "@skyleague/therefore": "^4.1.1",
    "@types/uuid": "^9.0.8",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "exports": {
    ".": "./.dist/index.js",
    "./package.json": "./package.json",
    "./*.js": "./.dist/*.js"
  },
  "workspaces": [
    "docs"
  ]
}